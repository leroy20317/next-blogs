{
  "name": "next-blogs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 5500 -H local.leroytop.com",
    "build": "next build",
    "start-local": "next start -p 5500 -H local.leroytop.com",
    "start": "next start -p 5500",
    "publish": "next build && node kodo.js",
    "pm2": "pm2 start npm --no-pmx --name next-blogs -- start",
    "type-check": "tsc",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.scss\" --syntax=scss",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\""
  },
  "lint-staged": {
    "**/*.scss": "npm run lint:style",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,scss,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@reduxjs/toolkit": "^1.7.2",
    "ahooks": "^3.7.8",
    "antd": "^5.11.1",
    "antd-style": "^3.5.2",
    "axios": "^1.6.2",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.10",
    "gitalk": "^1.8.0",
    "lodash": "^4.17.21",
    "next": "^13.5.6",
    "next-redux-wrapper": "^8.1.0",
    "node-abort-controller": "^3.0.1",
    "parallax-js": "^3.1.0",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.0",
    "react-redux": "^7.2.9",
    "react-syntax-highlighter": "^15.4.5",
    "redux": "^4.2.1",
    "sass": "^1.45.0",
    "yet-another-abortcontroller-polyfill": "^0.0.4"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.465.0",
    "@next/bundle-analyzer": "^12.0.7",
    "@types/lodash": "^4.14.178",
    "@types/node": "^20.9.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@umijs/fabric": "2.7.0",
    "co": "^4.6.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^14.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^12.1.2",
    "path": "^0.12.7",
    "prettier": "^2.5.1",
    "rd": "^2.0.1",
    "react-flip-move": "^3.0.4",
    "stylelint": "^13.13.1",
    "typescript": "^5.2.2"
  }
}
