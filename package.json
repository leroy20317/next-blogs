{
  "name": "next-blogs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next  -p 5500 -H local.leroy.net.cn",
    "build": "next build",
    "start": "next start -p 5500",
    "publish": "next build && node kodo.js",
    "pm2": "pm2 delete next-blogs && pm2 start npm --no-pmx --name next-blogs -- start",
    "type-check": "tsc",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.scss\" --syntax scss",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\""
  },
  "husky": {
    "hooks": {
      "commit-msg": "fabric verify-commit",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.scss": "stylelint --syntax scss",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.3",
    "@reduxjs/toolkit": "^1.6.1",
    "antd": "^4.16.12",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "next": "^11.1.0",
    "next-redux-wrapper": "^7.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "sass": "^1.38.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^11.1.0",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.6.2",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@umijs/fabric": "^2.6.4",
    "co": "^4.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-next": "^11.1.0",
    "husky": "^4.3.8",
    "lint-staged": "^11.1.2",
    "path": "^0.12.7",
    "prettier": "^2.3.2",
    "qiniu": "^7.3.2",
    "rd": "^2.0.1",
    "stylelint": "^13.13.1",
    "terser-webpack-plugin": "^5.1.4",
    "typescript": "^4.3.5"
  }
}
