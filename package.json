{
  "name": "next-blogs",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev -p 5500 -H local.leroytop.com",
    "build": "next build",
    "start-local": "next start -p 5500 -H local.leroytop.com",
    "start": "next start -p 5500",
    "publish": "next build",
    "pm2": "pm2 start pnpm --no-pmx --name next-blogs -- start",
    "type-check": "tsc",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint:js": "eslint . --fix",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.css\"",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "postinstall": "husky"
  },
  "lint-staged": {
    "**/*.scss": "npm run lint:style",
    "**/*.{js,jsx,ts,tsx}": "npm run lint:js",
    "**/*.{js,jsx,tsx,ts,css,scss,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.0",
    "@reduxjs/toolkit": "^2.5.1",
    "ahooks": "^3.8.4",
    "antd": "^5.23.3",
    "axios": "^1.7.9",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.10",
    "gitalk": "^1.8.0",
    "lodash-es": "^4.17.21",
    "next": "^15.1.6",
    "next-redux-wrapper": "^8.1.0",
    "next-seo": "^6.6.0",
    "parallax-js": "^3.1.0",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.3",
    "react-redux": "^9.2.0",
    "react-syntax-highlighter": "^15.4.5",
    "redux": "^5.0.1",
    "sass": "^1.45.0"
    },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.465.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/compat": "^1.1.0",
    "@eslint/js": "^9.6.0",
    "@types/crypto-js": "^4.2.2",
    "@types/eslint__js": "^8.42.3",
    "@next/bundle-analyzer": "^12.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "22.9.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "co": "^4.6.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "mime": "^4.0.3",
    "path": "^0.12.7",
    "autoprefixer": "^10.4.19",
    "commitlint": "^19.3.0",
    "eslint": "^9.6.0",
    "eslint-config-next": "^14.2.4",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "p-limit": "^6.1.0",
    "ping": "^0.4.2",
    "postcss": "^8.4.39",
    "prettier": "^3.4.2",
    "rd": "^2.0.1",
    "react-flip-move": "^3.0.4",
    "stylelint": "^16.6.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-config-standard": "^36.0.1",
